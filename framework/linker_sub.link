MEMORY {
	ram: org=0x6000, len=0xA000
}

SECTIONS {
	code: {
		INT_ProgramStart = .;
		*(header);
		. = ALIGN(2);
		*(main);
		. = ALIGN(2);
		*(code);
		. = ALIGN(2);
		*(data);
		. = ALIGN(0x20);
		INT_ProgramEnd    = .;
		INT_PRG_RAM_CLEAR = (0x80000-INT_ProgramEnd)/0x20;
	} > ram

	bss (NOLOAD): {
		*(bss);
	} > ram
}